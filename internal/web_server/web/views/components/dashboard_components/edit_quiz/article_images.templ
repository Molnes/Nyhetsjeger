package dashboard_components

import (
	"net/url"
)

templ ArticleImages(images []url.URL) {
	<ul class="flex flex-row justify-center flex-wrap w-full p-3 gap-5">
		for _, img := range images {
			<li>
				<img src={ img.String() } class="h-28 w-40 object-cover rounded-card"/>
			</li>
		}
		if len(images) == 0 {
			<li class="w-full text-center px-4 py-2 border border-clightindigo bg-purple-50 rounded-card">Fant ingen bilder</li>
		}
	</ul>
}
